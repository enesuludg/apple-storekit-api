{
  "name": "apple-storekit-api",
  "version": "1.0.11",
  "description": "Apple StoreKit API integration for In-App Purchases and Subscriptions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/appleStoreKit.js",
    "dist/appleStoreKit.d.ts",
    "dist/interfaces/*",
    "dist/services/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "dev": "ts-node examples/example.dev.ts",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --watch 'examples/**/*.ts' --exec 'ts-node' examples/example.dev.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "apple",
    "storekit",
    "in-app-purchase",
    "iap",
    "subscription",
    "ios",
    "app-store"
  ],
  "author": "Enes uludag",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^14.18.63",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enesuludg/apple-storekit-api.git"
  },
  "engines": {
    "node": ">=10.24.1"
  },
  "directories": {
    "example": "examples"
  },
  "bugs": {
    "url": "https://github.com/enesuludg/apple-storekit-api/issues"
  },
  "homepage": "https://github.com/enesuludg/apple-storekit-api#readme"
}
